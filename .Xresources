URxvt.iso14755 : false
Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

URxvt.termName: rxvt-unicode-256color
URxvt.font: xft:Hack:style=Book:antialias=true:size=12, \
            xft:WenQuanYi Bitmap Song:size=12, \
            xft:unifont:style=Medium:antialias=false, \
            xft:FreeSerif:style=Regular
URxvt*background: #151718
URxvt*foreground: #d6d6d6
URxvt*cursorColor: #d6d6d6
URxvt.perl-ext-common:  default,clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
